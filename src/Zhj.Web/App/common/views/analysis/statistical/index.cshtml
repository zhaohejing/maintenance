<div ng-controller="common.views.analysis.statistical.index as vm">
    <div class="row margin-bottom-5">
        <div class="col-xs-6">
            <div class="page-head">
                <div class="page-title">
                    <h1>
                        <span>检修统计</span> 
                    </h1>
                </div>
            </div>
        </div>
  
    </div>
    <div class="portlet light">
        <div class="portlet-body">
            <uib-tabset class="tab-container tabbable-line record-cont portlet light">
                <uib-tab heading="设备维修情况" ng-click="vm.devicerepair()">
                    <div class="portlet-body portlet-container3">
                        <!-- 角色列表 -->
                        <table object-table
                               data="vm.jTableInfo.data"
                               check-model="vm.jTableInfo.checkModel"
                               back-model="vm.jTableInfo.backModel"
                               display="vm.jTableInfo.pageConfig.itemsPerPage"
                               headers="设备名称,设备资产号,设备别名,设备厂商,设备负责人,设备标识,维修次数"
                               fields="deviceName,deviceAssetNo,deviceAlias,deviceManufacture,deviceHead,deviceSIgnkey,count">
                            <tbody>
                                <tr ng-click="$owner.vm.showeditinfo(item)">
                                    <td>
                                        <input type="checkbox"
                                               ng-model="::item.checked"
                                               ng-click="$event.stopPropagation();checkOne(item);" />
                                    </td>
                                    <td show-td item="item" name="deviceName"></td>
                                    <td show-td item="item" name="deviceAssetNo"></td>
                                    <td show-td item="item" name="deviceAlias"></td>
                                    <td show-td item="item" name="deviceManufacture"></td>
                                    <td show-td item="item" name="deviceHead"></td>
                                    <td show-td item="item" name="deviceSIgnkey"></td>
                                    <td show-td item="item" name="count"></td>
                                </tr>
                            </tbody>
                        </table>
                        <!--分页-->
                        <nav class="paging">
                            <obj-pagination conf="vm.jTableInfo.pageConfig"></obj-pagination>
                        </nav>
                    </div>
                </uib-tab>
                <uib-tab>
                    <uib-tab-heading heading="工程师检修情况" ng-click="vm.enginerrepair()">
                        工程师检修情况
                    </uib-tab-heading>
                    <div class="portlet-body portlet-container3">
                        <!-- 角色列表 -->
                        <table object-table
                               data="vm.tTableInfo.data"
                               check-model="vm.tTableInfo.checkModel"
                               back-model="vm.tTableInfo.backModel"
                               display="vm.tTableInfo.pageConfig.itemsPerPage"
                               headers="用户名,姓名,邮箱,维修次数,进行中,已完成"
                               fields="userName,name,emailAddress,fullSolve,solving,hadSolve">
                            <tbody>
                                <tr ng-click="$owner.vm.showeditinfo(item)">
                                    <td>
                                        <input type="checkbox"
                                               ng-model="::item.checked"
                                               ng-click="$event.stopPropagation();checkOne(item);" />
                                    </td>
                                    <td show-td item="item" name="userName"></td>
                                    <td show-td item="item" name="name"></td>
                                    <td show-td item="item" name="emailAddress"></td>
                                    <td show-td item="item" name="fullSolve"></td>
                                    <td show-td item="item" name="solving"></td>
                                    <td show-td item="item" name="hadSolve"></td>
                                </tr>
                            </tbody>
                        </table>
                        <!--分页-->
                        <nav class="paging">
                            <obj-pagination conf="vm.tTableInfo.pageConfig"></obj-pagination>
                        </nav>
                    </div>
                </uib-tab>
            </uib-tabset>
        </div>
    </div>
</div>